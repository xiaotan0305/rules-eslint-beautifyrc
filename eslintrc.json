{
    "parser": "babel-eslint",
    "plugins": [
        "node",
        "json",
        "vue",
        "html"
    ],
    "globals": {
        "seajs": true,
        "wx": true,
        "App": true,
        "getApp": true,
        "Page": true,
        "getCurrentPages": true,
        "Vue": true,
        "fangimjs": true,
        "THREE": true,
        "layui": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "amd": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended"
    ],
    "rules": {
        "node/exports-style": [
            "error",
            "module.exports"
        ],
        "node/no-unsupported-features": 0,
        "node/no-unsupported-features/es-syntax": 0,
        "no-alert": 0,
        "no-array-constructor": 1,
        "no-bitwise": 0,
        "no-caller": 1,
        "no-catch-shadow": 1,
        "no-class-assign": 1,
        "no-cond-assign": 1,
        "no-console": 0,
        "no-const-assign": 1,
        "no-constant-condition": 1,
        "no-continue": 0,
        "no-control-regex": 1,
        "no-debugger": 0,
        "no-delete-var": 1,
        "no-div-regex": 1,
        "no-dupe-keys": 1,
        "no-dupe-args": 1,
        "no-duplicate-case": 1,
        "no-else-return": 1,
        "no-empty": 1,
        "no-empty-character-class": 1,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-ex-assign": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": 1,
        "no-extra-semi": 1,
        "no-fallthrough": 0,
        "no-floating-decimal": 1,
        "no-func-assign": 1,
        "no-implicit-coercion": [
            2,
            {
                "boolean": false,
                "number": false,
                "string": false
            }
        ],
        "no-implied-eval": 1,
        "no-inline-comments": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 1,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 0,
        "no-iterator": 1,
        "no-label-var": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-lonely-if": 1,
        "no-loop-func": 1,
        "no-mixed-requires": [
            1,
            true
        ],
        "no-mixed-spaces-and-tabs": 1,
        "linebreak-style": 0,
        "no-multi-spaces": [
            2,
            {
                "exceptions": {
                    "BinaryExpression": true
                }
            }
        ],
        "no-multi-str": 1,
        "no-multiple-empty-lines": 1,
        "no-native-reassign": 1,
        "no-negated-in-lhs": 1,
        "no-nested-ternary": 1,
        "no-new": 0,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-wrappers": 1,
        "no-obj-calls": 1,
        "no-octal": 1,
        "no-octal-escape": 1,
        "no-param-reassign": 0,
        "no-path-concat": 1,
        "no-plusplus": 0,
        "no-process-env": 0,
        "no-process-exit": 0,
        "no-case-declarations": 0,
        "no-proto": 1,
        "no-redeclare": [
            2,
            {
                "builtinGlobals": true
            }
        ],
        "no-regex-spaces": 1,
        "no-restricted-modules": 1,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-shadow": 0,
        "no-shadow-restricted-names": 1,
        "no-spaced-func": 1,
        "no-sparse-arrays": 1,
        "no-sync": 0,
        "no-ternary": 0,
        "no-trailing-spaces": [
            2,
            {
                "skipBlankLines": true
            }
        ],
        "no-this-before-super": 1,
        "no-throw-literal": 1,
        "no-undef": 2,
        "no-undef-init": 1,
        "no-undefined": 0,
        "no-unexpected-multiline": 1,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 1,
        "no-unreachable": 2,
        "no-unused-expressions": [
            2,
            {
                "allowShortCircuit": true,
                "allowTernary": true
            }
        ],
        "no-unused-vars": 2,
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "no-useless-call": 0,
        "no-void": 1,
        "no-var": 0,
        "no-warning-comments": 0,
        "no-with": 1,
        "array-bracket-spacing": [
            2,
            "never"
        ],
        "arrow-parens": 0,
        "arrow-spacing": 1,
        "accessor-pairs": 0,
        "block-scoped-var": 1,
        "brace-style": 1,
        "callback-return": 0,
        "camelcase": [
            0,
            {
                "properties": "never"
            }
        ],
        "comma-dangle": 0,
        "comma-spacing": 1,
        "comma-style": 1,
        "complexity": 0,
        "computed-property-spacing": 1,
        "consistent-return": 0,
        "consistent-this": [
            2,
            "that"
        ],
        "constructor-super": 1,
        "curly": [
            2,
            "multi-line"
        ],
        "default-case": 0,
        "dot-location": [
            2,
            "property"
        ],
        "dot-notation": 0,
        "eol-last": 0,
        "eqeqeq": 1,
        "func-names": 0,
        "func-style": 0,
        "generator-star-spacing": 0,
        "guard-for-in": 1,
        "handle-callback-err": 1,
        "id-length": [
            2,
            {
                "min": 1,
                "max": 30
            }
        ],
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1,
                "VariableDeclarator": 1
            }
        ],
        "init-declarations": 0,
        "key-spacing": 1,
        "lines-around-comment": 0,
        "max-depth": [
            2,
            10
        ],
        "max-len": 0,
        "max-nested-callbacks": [
            2,
            3
        ],
        "max-params": 0,
        "max-statements": 0,
        "new-cap": 0,
        "new-parens": 1,
        "newline-after-var": 0,
        "object-curly-spacing": [
            2,
            "always",
            {
                "objectsInObjects": false,
                "arraysInObjects": false
            }
        ],
        "object-shorthand": 0,
        "one-var": 0,
        "operator-assignment": [
            2,
            "always"
        ],
        "operator-linebreak": [
            2,
            "before"
        ],
        "padded-blocks": [
            1,
            "never"
        ],
        "prefer-const": 0,
        "prefer-spread": 0,
        "prefer-reflect": 0,
        "quote-props": [
            2,
            "as-needed"
        ],
        "quotes": [
            2,
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "radix": 0,
        "id-match": 0,
        "require-yield": 0,
        "semi": [
            2,
            "always"
        ],
        "semi-spacing": 0,
        "sort-vars": 0,
        "space-before-blocks": 1,
        "space-before-function-paren": [
            2,
            {
                "anonymous": "never",
                "named": "never",
                "asyncArrow": "never"
            }
        ],
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "space-unary-ops": 0,
        "spaced-comment": [
            2,
            "always"
        ],
        "strict": [
            2,
            "function"
        ],
        "use-isnan": 1,
        "valid-jsdoc": 0,
        "valid-typeof": 1,
        "vars-on-top": 0,
        "wrap-iife": [
            2,
            "inside"
        ],
        "wrap-regex": 0,
        "yoda": 0
    }
}